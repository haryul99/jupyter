    "        print('\\n정답입니다.')\n",
    "        break    \n",
    "    else :\n",
    "        chance -= 1\n",
    "        print('\\n정답이 아닙니다.')\n",
    "        if question > answer1 :\n",
    "            print('\\n\\t down!\\n')\n",
    "            print('맞출기회가%d번남았습니다\\n'%chance)\n",
    "        else :\n",
